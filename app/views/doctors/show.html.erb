<h1><%= @doctor.full_name %></h1>
<hr />
<h2>Patients</h2>
<ul>
  <% @doctor.patients.each do |patient| %>
    <li>
      <%= link_to patient.full_name, patient %>
    </li>
  <% end %>
</ul>
<br />
<br />
<br />
<%= link_to 'Delete Doctor', @doctor, method: :delete, class: "ui red button inverted" %>